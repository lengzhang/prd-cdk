{
  "name": "prd-cdk",
  "description": "This is a CDK project for AWS resources and services deployment.",
  "version": "0.1.0",
  "scripts": {
    "build": "tsc && npm run test && cdk ls",
    "watch": "tsc -w",
    "test": "jest --verbose",
    "clean": "rm -rf dist cdk.out",
    "deploy": "cdk deploy --all --require-approval=never",
    "destroy": "yes | cdk destroy --all"
  },
  "devDependencies": {
    "@aws-cdk/aws-cognito-identitypool-alpha": "^2.103.1-alpha.0",
    "@types/jest": "^29.5.5",
    "@types/node": "20.7.1",
    "aws-cdk": "2.103.1",
    "dotenv": "^16.3.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "~5.2.2"
  },
  "dependencies": {
    "aws-cdk-lib": "2.103.1",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
  }
}
